{#
/**
 * @file deduction.html.twig
 * Default theme implementation to present Deduction data.
 *
 * This template is used when viewing Deduction pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_deduction()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('deduction') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
